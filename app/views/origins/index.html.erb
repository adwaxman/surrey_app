<p id="notice"><%= notice %></p>

<h1>Listing Origins</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Address line1</th>
      <th>Address line2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Primary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @origins.each do |origin| %>
      <tr>
        <td><%= origin.client %></td>
        <td><%= origin.address_line1 %></td>
        <td><%= origin.address_line2 %></td>
        <td><%= origin.city %></td>
        <td><%= origin.state %></td>
        <td><%= origin.zip %></td>
        <td><%= origin.primary %></td>
        <td><%= link_to 'Show', origin %></td>
        <td><%= link_to 'Edit', edit_origin_path(origin) %></td>
        <td><%= link_to 'Destroy', origin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Origin', new_origin_path %>
